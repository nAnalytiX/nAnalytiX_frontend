{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:react-hooks/recommended",
		"eslint-config-prettier"
	],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"plugins": [
		"react",
		"react-hooks",
		"eslint-plugin-prettier"
	],
	"rules": {
		"prettier/prettier": 2,
		"react/prop-types": 1,
		"react-hooks/exhaustive-deps": 1
	}
}
